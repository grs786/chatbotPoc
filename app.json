{
  "expo": {
    "name": "ChatApp",
    "slug": "ChatApp",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "jsEngine": "hermes",
      "userInterfaceStyle": "light",
      "supportsTablet": false,
      "bundleIdentifier": "com.ford.diagnowbot",
      "infoPlist": {
        "NSMicrophoneUsageDescription": "This app needs access to the microphone to record audio messages.",
        "NSPhotoLibraryUsageDescription": "This app want to use the photo library permission for app attachment purpose.",
        "NSPhotoLibraryAddUsageDescription": "This app want to use the photo library permission for app attachment purpose."
      },
      "buildNumber": "7"
    },
    "android": {
      "userInterfaceStyle": "light",
      "package": "com.ford.diagnowbot",
      "usesCleartextTraffic": true,
      "permissions": [
        "RECORD_AUDIO",
        "READ_EXTERNAL_STORAGE",
        "WRITE_EXTERNAL_STORAGE",
        "INTERNET"
      ],
      "jsEngine": "hermes",
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "versionCode": "10"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e2ca9b37-a49a-4fa3-a187-39988b1d2629"
      }
    },
    "owner": "gauravinfo"
  }
}
